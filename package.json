    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "chai": "4.3.4",
    "coveralls": "3.0.9",
    "it-each": "0.4.0",
    "mocha": "9.0.2",
    "nyc": "15.1.0",
    "typescript": "4.3.5"
